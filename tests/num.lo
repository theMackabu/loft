fn main() {
   io::println(42.type_name());          // integer without type suffix
   io::println(42i32.type_name());       // 32-bit signed integer
   io::println(42u64.type_name());       // 64-bit unsigned integer
   io::println(3.14.type_name());        // float without type suffix
   io::println(3.14f32.type_name());     // 32-bit float
   io::println(3.14f64.type_name());     // 64-bit float
   io::println(1e-10.type_name());       // scientific notation
   io::println(1.23e+4f32.type_name());  // scientific notation with type suffix
}
