fn main() {   
   test_eq!(42.type_name(), "i32");          // integer without type suffix
   test_eq!(3.1.type_name(), "f64");         // float without type suffix
   
   test_eq!(42i32.type_name(), "i32");       // 32-bit signed integer
   test_eq!(42u64.type_name(), "u64");       // 64-bit unsigned integer
   
   test_eq!(3.14f32.type_name(), "f32");     // 32-bit float
   test_eq!(3.14f64.type_name(), "f64");     // 64-bit float
   
   test_eq!(1e-10.type_name(), "f64");       // scientific notation
   test_eq!(1.23e+4f32.type_name(), "f32");  // scientific notation with type suffix
}
