fn test_assignment(val: &mut i32) -> &mut i32 {
   *val += 3;
   
   println!("value(2) is: {val}");   
   return val;
}

fn main() -> Result<(), &str> {
   let mut value = 5;

   println!("value(1) is: {value}");

   let val2 = test_assignment(&mut value);
   *val2 += 10;
      
   println!("value(3) is: {value}");
      
   Result::Ok(())
}

