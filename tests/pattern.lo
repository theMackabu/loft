fn main() {
   let success = Ok(42);
   let failure = Err("error message");
   let maybe = Some(123);
   let nothing = None;
   
   let result = match success {
      Ok(n) => n,
      Err(_) => -1,
   };
   
   let error_msg = match failure {
      Ok(_) => "success",
      Err(msg) => msg,
   };
   
   let opt_value = match maybe {
      Some(x) => x,
      None => 0,
   };
   
   let default = match nothing {
      Some(x) => x,
      None => 999,
   };
   
   let value = match 0 {
      0i32 => "zero",
      0u8 => "unsigned zero",
      1.0f32 => "one float",
      -1i64 => "negative one",
      -1.0f64 => "negative one float",
      n if n < 0 => "negative",
      _ => "positive"
   };
   
   success.unwrap_or(32);
   
   test_eq!(result, 42);
   test_eq!(error_msg, "error message",);
   test_eq!(opt_value, 123);
   test_eq!(default, 999);
   test_eq!(value, "zero");
}