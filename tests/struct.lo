struct Info {
   id: u8,
   name: &str, 
}

impl Info {
   fn get_name(&self) -> &str {
      self.name
   }
   
   fn set_name(&mut self, name: &str) {
      self.name = name;
   }
}

fn main() {
    let mut info = Info { id: 1, name: "first" };
    io::println(info.get_name());
    info.set_name("third");
    io::println(info.get_name());
}
