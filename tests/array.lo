fn analyze(slice: [i32]) {
   println!("First element of the slice: {}", slice[0]);
   // println!("The slice has {} elements", slice.len());
   println!();
}

fn main() {   
   let mut s: [i32] = [1, 2, 3];
   let xs: [i32; 5] = [1, 2, 3, 4, 5];
   let ys: [&str; 500] = [":3"; 500];

   s[0] += 8;
   
   analyze(s);
   analyze(xs);
   analyze(ys[1 .. 4]);
   
   let val = 1;
   match xs.get(val) {
      Some(val) => println!("The found value is {val}"),
      None => println!("Cannot find {val} in xs"),
   }
}